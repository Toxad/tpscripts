# Create image and move it 
if [ "`command -v pngtopnm`" ] \
&& [ "`command -v ppmquant`" ] \
&& [ "`command -v pnmtoplainpnm`" ]; then
    echo "bom?"
else
    echo "ruim?"
fi

exit $?
